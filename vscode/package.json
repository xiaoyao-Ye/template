{
  "publisher": "_Ghosteye",
  "name": "name-id",
  "displayName": "search extensions by name",
  "version": "0.0.0",
  "private": true,
  "description": "current extension description",
  "author": "_Ghosteye <Ghosteye@yeah.net>",
  "license": "MIT",
  "homepage": "https://github.com/xiaoyao-Ye/template#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaoyao-Ye/template"
  },
  "bugs": {
    "url": "https://github.com/xiaoyao-Ye/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "public/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "pnpm run build --watch",
    "vscode:prepublish": "pnpm run test && pnpm run build",
    "publish": "bumpp && vsce publish --no-dependencies",
    "pack": "pnpm run build && vsce package --no-dependencies",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/vscode": "^1.78.1",
    "@vscode/vsce": "^2.19.0",
    "bumpp": "^9.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.2.3",
    "vitest": "^0.31.4"
  }
}
